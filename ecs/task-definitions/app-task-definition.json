          "valueFrom": "arn:aws:secretsmanager:sa-east-1:181684851258:secret:users-service-secrets-UcADSr:GITHUB_CLIENT_SECRET::"
        },
        {
          "name": "GOOGLE_CLIENT_ID",
          "valueFrom": "arn:aws:secretsmanager:sa-east-1:181684851258:secret:users-service-secrets-UcADSr:GOOGLE_CLIENT_ID::"
        },
        {
          "name": "GOOGLE_CLIENT_REDIRECT_URI",
          "valueFrom": "arn:aws:secretsmanager:sa-east-1:181684851258:secret:users-service-secrets-UcADSr:GOOGLE_CLIENT_REDIRECT_URI::"
