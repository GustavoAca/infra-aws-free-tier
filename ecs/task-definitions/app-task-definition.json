        },
        {
          "name": "PUBLIC_KEY",
          "valueFrom": "arn:aws:secretsmanager:sa-east-1:181684851258:secret:users-service-secrets-UcADSr:PUBLIC_KEY::"
        },
        {
          "name": "PORT",
          "valueFrom": "arn:aws:secretsmanager:sa-east-1:181684851258:secret:users-service-secrets-UcADSr:PORT::"
        },
        {
          "name": "SERVICOS_EXTERNOS_PERMITIDOS",
          "valueFrom": "arn:aws:secretsmanager:sa-east-1:181684851258:secret:users-service-secrets-UcADSr:SERVICOS_EXTERNOS_PERMITIDOS::"
        }
      ],
      "healthCheck": {
        "command": [
          "CMD-SHELL",
          "curl -f http://localhost:8081/users/actuator/health || exit 1"
        ],
        "interval": 30,
        "timeout": 5,
        "retries": 3,
        "startPeriod": 60
      },
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "ecs-user-service",
          "awslogs-region": "sa-east-1",
          "awslogs-stream-prefix": "ecs"
        }
