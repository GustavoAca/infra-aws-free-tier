        },
        {
          "name": "PASSWORD_DB_FLYWAY",
          "valueFrom": "arn:aws:secretsmanager:sa-east-1:181684851258:secret:users-service-secrets-UcADSr:PASSWORD_DB_FLYWAY::"
        },
        {
          "name": "GITHUB_CLIENT_ID",
          "valueFrom": "arn:aws:secretsmanager:sa-east-1:181684851258:secret:users-service-secrets-UcADSr:GITHUB_CLIENT_ID::"
