{
  "family": "rds-bootstrap",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["EC2"],
  "cpu": "256",
  "memory": "256",
  "executionRoleArn": "arn:aws:iam::181684851258:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::181684851258:role/ecs-task-role-app",
  "containerDefinitions": [
    {
      "name": "rds-bootstrap",
      "image": "181684851258.dkr.ecr.sa-east-1.amazonaws.com/rds-bootstrap:latest",
      "essential": true,
      "secrets": [
        {
          "name": "DB_HOST",
          "valueFrom": "arn:aws:secretsmanager:sa-east-1:181684851258:secret:rds-bootstrap-secrets-n8qsal:DB_HOST::"
        },
        {
          "name": "DB_NAME",
          "valueFrom": "arn:aws:secretsmanager:sa-east-1:181684851258:secret:rds-bootstrap-secrets-n8qsal:DB_NAME::"
        },
        {
          "name": "DB_USER",
          "valueFrom": "arn:aws:secretsmanager:sa-east-1:181684851258:secret:rds-bootstrap-secrets-n8qsal:DB_USER::"
        },
        {
          "name": "PGPASSWORD",
          "valueFrom": "arn:aws:secretsmanager:sa-east-1:181684851258:secret:rds-bootstrap-secrets-n8qsal:DB_PASSWORD::"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "ecs-rds-bootstrap",
          "awslogs-region": "sa-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}